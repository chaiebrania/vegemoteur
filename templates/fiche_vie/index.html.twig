{% extends 'base.html.twig' %}

{% block title %}FicheVie index{% endblock %}

{% block body %}
    <h1>Fiche De Vie</h1>
<div class="card-block">
   <div class="table-responsive dt-responsive">
      <table id="simpletable" class="table table-striped table-bordered nowrap">
   
        <thead>
            <tr>
            
                <th>Nature de l'intervention</th>
                <th>Ref.Document</th>
                <th>Resultat</th>
                <th>Visa</th>
                 <th>Date</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for fiche_vie in fiche_vies %}
            <tr>
            
                <td>{{ fiche_vie.nateintervention }}</td>
                <td>{{ fiche_vie.refdocument }}</td>
                <td>{{ fiche_vie.resultat }}</td>
                <td>{{ fiche_vie.visa }}</td>
                <td>{{ fiche_vie.date ? fiche_vie.date|date('Y-m-d H:i:s') : '' }}</td>

                <td>
                    <a href="{{ path('fiche_vie_show', {'id': fiche_vie.id}) }}" class="btn btn-info">detail</a>
                    <a href="{{ path('fiche_vie_edit', {'id': fiche_vie.id}) }}" class="btn btn-grd-warning ">Modifier</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="7">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>
   
       </div>
      
  </div>
 </div>
     <a href="{{ path('fiche_vie_new') }}" class="btn btn-primary">Ajouter</a>

  
     
     
{% endblock %}
