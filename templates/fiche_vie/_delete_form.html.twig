<form method="post" action="{{ path('fiche_vie_delete', {'id': fiche_vie.id}) }}" onsubmit="return confirm('Are you sure you want to delete this item?');">
    <input type="hidden" name="_method" value="DELETE">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ fiche_vie.id) }}">
    <button class="btn btn-danger">Supprimer</button>
</form>
