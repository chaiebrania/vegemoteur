               <a class="mobile-menu" id="mobile-collapse" href="#!">
               <i class="feather icon-menu"></i>
               </a>
               <a href="index.html">
               <img class="img-fluid" src="{{asset('assets/images/vege1.png')}}" alt="Theme-Logo" />
               </a>
