                        <span class="pcoded-mtext">Fiche Calibrage</span>
                        </a>
                     </li>
                  </ul>
               </div>
            </nav>
            <div class="pcoded-content">
               <div class="pcoded-inner-content">
                  <div class="main-body">
                     {% block body %}{% endblock %}      
                  </div>
               </div>
            </div>
         </div>
      </div>
   </div>
</div>
      
        {% block javascripts %}

        <script src="{{asset('assets/js/jquery.min.js')}}"></script>
<script src="{{asset('assets/js/jquery-ui.min.js')}}"></script>
